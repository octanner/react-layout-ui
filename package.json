{
  "version": "0.1.0",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test --passWithNoTests",
    "lint": "tsdx lint",
    "prettier": "prettier --write",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "prepare": "tsdx build"
  },
  "peerDependencies": {
    "react": ">=16"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,md,html,css}": [
      "prettier --write",
      "git add ."
    ],
    "*.ts?(x)": [
      "yarn lint",
      "yarn typecheck"
    ]
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "all"
  },
  "name": "react-layout-ui",
  "author": "Tyler Haas",
  "module": "dist/react-layout-ui.esm.js",
  "devDependencies": {
    "@types/jest": "^25.2.1",
    "@types/react": "^16.9.32",
    "@types/react-dom": "^16.9.6",
    "husky": "^4.2.3",
    "lint-staged": "^10.1.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "tsdx": "^0.13.1",
    "tslib": "^1.11.1",
    "typescript": "^3.8.3"
  }
}
